{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/backend/api_server.py"
        },
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "functions": {
        "backend/api_server.py": {
            "maxDuration": 10
        }
    }
}